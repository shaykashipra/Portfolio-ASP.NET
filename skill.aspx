             <!-- Left side technical skills -->
                <div class="skill-left">
                    <h3>Technical Skills</h3>
                   

                    

                    



              
                     



     <% foreach (var skill in TechSkillsData) { %>
        <div class="skill-bar">
            <div class="info-skill">
                <p><%= skill.Title %></p>
                <p><%= skill.Percentage %>%</p>
            </div>
